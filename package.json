{
  "name": "grim",
  "version": "2.0.3",
  "description": "Log usage of deprecated methods",
  "main": "./lib/grim",
  "scripts": {
    "lint": "coffeelint .",
    "test": "npm run build && jasmine-node --coffee --captureExceptions --forceexit spec",
    "dev": "npm run build -- --watch",
    "build": " babel src --out-dir lib --delete-dir-on-start",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atom/grim.git"
  },
  "bugs": {
    "url": "https://github.com/atom/grim/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/atom/grim/raw/master/LICENSE.md"
    }
  ],
  "dependencies": {
    "event-kit": "^2.0.0"
  },
  "devDependencies": {
    "babel-preset-atomic": "^4.1.0",
    "coffeelint": "^2.1.0",
    "jasmine-node": "^3.0.0",
    "temp": "^0.6.0"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "level": "error",
      "value": 120
    }
  }
}
